{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "enemy_schema.json",
  "title": "Enemy Schema",
  "description": "Schema for the 'enemy' property.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "strength": {
      "type": "integer"
    },
    "toughness": {
      "type": "integer"
    },
    "hurt": {
      "type": "integer"
    }
  },
  "required": ["name", "strength", "toughness"]
}
